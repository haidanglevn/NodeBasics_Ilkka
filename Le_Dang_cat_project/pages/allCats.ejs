<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>All Cats</title>
    <!--     <link rel="stylesheet" href="styles/basicStyles.css">
 -->
  </head>
  <body>
    <main>
      <h2>All Cats</h2>
      <table>
        <thead>
          <tr>
            <th>Cat Number</th>
            <th>Name</th>
            <th>Year of Birth</th>
            <th>Breed</th>
            <th>Length</th>
          </tr>
        </thead>
        <tbody>
          <% result.forEach(cat => { %>
          <tr>
            <td><%= cat.number %></td>
            <td><%= cat.name %></td>
            <td><%= cat.yearOfBirth %></td>
            <td><%= cat.breed %></td>
            <td><%= cat.length %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
      <a href="/"><button>Home</button></a>
    </main>
  </body>
</html>
